{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "node_modules",
      "**/out"
    ],
    "runtime": "nodejs12"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
